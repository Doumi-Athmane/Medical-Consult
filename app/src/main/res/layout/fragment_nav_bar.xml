<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.view.fragments.NavBarFragment"
    android:id="@+id/frameLayout" >

    <ImageView
        android:id="@+id/ic_traitement"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginEnd="40dp"
        android:src="@drawable/traitement"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/ic_medecins"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginTop="600dp"
        android:layout_marginBottom="39dp"
        android:src="@drawable/medecins"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:src="@drawable/rdv"
        app:layout_constraintBottom_toBottomOf="@+id/ic_medecins"
        app:layout_constraintEnd_toStartOf="@+id/ic_traitement"
        app:layout_constraintHorizontal_bias="0.713"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ic_medecins"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/TMedecins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Medecins"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ic_medecins"
        app:layout_constraintStart_toStartOf="@+id/ic_medecins"
        app:layout_constraintTop_toBottomOf="@+id/ic_medecins" />

    <TextView
        android:id="@+id/TTraitement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Traitements"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ic_traitement"
        app:layout_constraintStart_toStartOf="@+id/ic_traitement"
        app:layout_constraintTop_toBottomOf="@+id/ic_traitement" />

    <TextView
        android:id="@+id/TRDV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rendez-vous"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />
</androidx.constraintlayout.widget.ConstraintLayout>